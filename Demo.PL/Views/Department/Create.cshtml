@using Demo.DAL.Entities
@model Department
@{
    ViewData["Title"] = "Create";
}

<h1>Create New Dep</h1>

<form method="post" asp-action="Create">
    <div class="form-groupg">
        <label class="col-form-label" asp-for="Code"></label>
        <input class="form-control" asp-for="Code" />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>


    <div class="form-groupg">
        <label class="col-form-label" asp-for="Name"></label>
        <input class="form-control"  asp-for="Name"/>
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>


    <div class="form-groupg">
        <label class="col-form-label" asp-for="CreatedAt"></label>
        <input class="form-control" asp-for="CreatedAt"/>
        <span asp-validation-for="CreatedAt" class="text-danger"></span>
    </div>
   <div class="mt-3">
        <input type="submit" value="Submit" class="btn btn-success" />
        <a asp-action="Index" class="btn btn-secondary ">Back To List</a>
   </div>

</form>
@section Validation {

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>

    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}