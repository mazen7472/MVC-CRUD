
@model Department
@{
    ViewData["Title"] = "Details";
}

<h1>@Model.Name Department Details</h1>


    <div class="form-groupg">
        <label class="col-form-label" asp-for="Code"></label>
        <input readonly class="form-control" asp-for="Code" />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>


    <div class="form-groupg">
        <label class="col-form-label" asp-for="Name"></label>
        <input readonly class="form-control" asp-for="Name" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>


    <div class="form-groupg">
        <label class="col-form-label" asp-for="CreatedAt"></label>
        <input readonly class="form-control" asp-for="CreatedAt" />
        <span asp-validation-for="CreatedAt" class="text-danger"></span>
    </div>
    <div class="mt-3">
    <a  asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-outline-success">Edit</a>
        <a asp-action="Index" class="btn btn-secondary ">Back To List</a>
    </div>



